{
  "id": "rs-geo-analytics",
  "name": "Rankscale GEO Analytics",
  "version": "1.0.0",
  "description": "Fetch and interpret Rankscale GEO (Generative Engine Optimization) analytics. Pulls brand visibility score, citation rate, sentiment, and top AI search terms. Generates actionable GEO insights using 7 interpretation rules.",
  "author": "sonicClaw@rankscale.ai",
  "brand": "Rankscale",
  "ticket": "RS-126",
  "entrypoint": "rankscale-skill.js",
  "runtime": "node",
  "nodeVersion": ">=16",
  "triggers": [
    "rankscale",
    "geo analytics",
    "geo report",
    "geo insights",
    "geo score",
    "ai search visibility",
    "ai visibility",
    "citation analysis",
    "citation rate",
    "brand sentiment ai",
    "ai search terms",
    "search term report",
    "brand visibility report"
  ],
  "credentials": [
    {
      "key": "RANKSCALE_API_KEY",
      "description": "Rankscale API key (format: rk_<hash>_<brandId>)",
      "required": true,
      "link": "https://app.rankscale.ai/settings/api"
    },
    {
      "key": "RANKSCALE_BRAND_ID",
      "description": "Rankscale Brand ID (extracted from API key if omitted)",
      "required": false,
      "note": "Auto-extracted from API key suffix if not set"
    }
  ],
  "capabilities": [
    "geo_score",
    "citation_analysis",
    "sentiment_analysis",
    "search_terms",
    "brand_discovery",
    "geo_interpretation"
  ],
  "endpoints": [
    "GET /v1/metrics/brands",
    "GET /v1/metrics/report",
    "GET /v1/metrics/citations",
    "GET /v1/metrics/sentiment",
    "GET /v1/metrics/search-terms-report"
  ],
  "output": {
    "format": "ascii",
    "width": 55,
    "mobile_compatible": true
  },
  "references": [
    "references/api-integration.md",
    "references/geo-playbook.md"
  ],
  "assets": [
    "assets/onboarding.md"
  ],
  "repository": "https://github.com/Mathias-RS/RS-Skill",
  "branch": "feature/rs-126-geo-analytics-skill",
  "tags": [
    "analytics",
    "geo",
    "seo",
    "ai-search",
    "rankscale",
    "brand",
    "visibility"
  ],
  "clawhhub": {
    "category": "Analytics",
    "subcategory": "GEO / AI Search",
    "icon": "chart-bar",
    "color": "#6366F1",
    "onboarding": "assets/onboarding.md"
  }
}
